{
  "name": "nmr-correlation",
  "version": "1.2.0",
  "description": "Build and manipulation of correlations from 1D and 2D NMR data",
  "main": "./lib/index.js",
  "module": "./lib-esm/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "src",
    "lib",
    "lib-esm"
  ],
  "scripts": {
    "build": "npm run tsc",
    "clean": "rimraf lib lib-esm",
    "eslint": "eslint src --ext js,ts --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "test": "npm run test-coverage && npm run eslint",
    "test-coverage": "jest --coverage",
    "tsc": "npm run clean && npm run tsc-cjs && npm run tsc-esm",
    "tsc-cjs": "tsc --project tsconfig.cjs.json",
    "tsc-esm": "tsc --project tsconfig.esm.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaelwenk/nmr-correlation.git"
  },
  "author": "Michael Wenk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michaelwenk/nmr-correlation/issues"
  },
  "homepage": "https://github.com/michaelwenk/nmr-correlation#readme",
  "devDependencies": {
    "@types/lodash": "^4.14.168",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-cheminfo-typescript": "^8.0.8",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.45.2",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
