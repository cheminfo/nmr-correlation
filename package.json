{
  "name": "nmr-correlation",
  "version": "2.2.4",
  "description": "Build and manipulation of correlations from 1D and 2D NMR data",
  "main": "./lib/index.js",
  "module": "./lib-esm/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "src",
    "lib",
    "lib-esm"
  ],
  "scripts": {
    "build": "npm run tsc",
    "clean": "rimraf lib lib-esm",
    "eslint": "eslint src --ext js,ts --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "prepack": "npm run build",
    "test": "npm run test-coverage && npm run eslint",
    "test-coverage": "jest --coverage",
    "tsc": "npm run clean && npm run tsc-cjs && npm run tsc-esm",
    "tsc-cjs": "tsc --project tsconfig.cjs.json",
    "tsc-esm": "tsc --project tsconfig.esm.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/nmr-correlation.git"
  },
  "author": "Michael Wenk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo/nmr-correlation/issues"
  },
  "homepage": "https://github.com/cheminfo/nmr-correlation#readme",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.178",
    "eslint": "^8.9.0",
    "eslint-config-cheminfo-typescript": "^10.3.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "cheminfo-types": "^1.0.0",
    "lodash": "^4.17.21",
    "nmr-processing": "^7.0.1"
  }
}
