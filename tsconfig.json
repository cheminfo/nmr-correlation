{
  "compilerOptions": {
    "esModuleInterop": true,
    "moduleResolution": "node",
    "inlineSourceMap": true,
    "strict": true,
    "target": "es2019",
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": [
      "./node_modules/@types"
    ]
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}